{

    "mongoConfig": {
        "host": "hmda-pilotdb.ec2-int.devis.com",
        "port": 27017,
        "database": "hmda"
    },

    "middleware": {

        "static": {
            "module": {
                "arguments": [ "path:./.build" ]
            }
        }

        ,"router": {
            "module": {
                "arguments": [{ "directory": "path:./controllers" }]
            }
        }


        ,"expressView": {
            "priority": 100,
            "enabled": true,
            "module": {
                "name": "engine-munger/lib/expressView"
            }
        }



    }
}
