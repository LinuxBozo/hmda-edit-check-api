{

    "mongoConfig": {
        "host": "hmda-pilotdb-dev.ec2-int.devis.com",
        "port": 27017,
        "database": "hmda"
    },

    "middleware": {

        "devtools": {
            "enabled": true,
            "priority": 35,
            "module": {
                "name": "kraken-devtools",
                "arguments": [
                    "path:./public",
                    "path:./.build",
                    {
                        "copier": {
                            "module": "kraken-devtools/plugins/copier",
                            "files": "**/*"
                        }
                    }
                ]
            }
        }
    }
}
